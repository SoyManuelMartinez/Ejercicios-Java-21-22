Algoritmo Ejercicio4parte4
	
	Escribir "Dime un numero"
	Leer primernum
	Escribir "Dime otro numero"
	Leer segundnum
	Si primernum >= 0 y segundnum >= 0
		Escribir "Tanto ", primernum, " como ", segundnum, " son positivos o iguales a 0"
	SiNo
		Si primernum < 0 y segundnum < 0
			Escribir "Tanto ", primernum, " como ", segundnum, " son negativos"
		SiNo
			Escribir  "Solamente existe un número positivo"
		FinSi
	FinSi
	
FinAlgoritmo
